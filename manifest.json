{
    "manifest_version": 2,
    "name": "Anna's Search",
    "description": "Adds a search engine that searches Anna's Archive",
    "developer": {
        "name": "Redshift Team",
        "url": ""
    },
    "version": "1.0",
    "chrome_settings_overrides": {
        "search_provider": {
            "name": "Anna's Search",
            "search_url": "https://annas-archive.org/search?q={searchTerms}",
            "keyword": "@annas",
            "favicon_url": "icons/48.png",
            "is_default": false,
            "encoding": "UTF-8"
        }
    },
    "permissions": [
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "icons": {
        "16": "icons/16.png",
        "24": "icons/24.png",
        "32": "icons/32.png",
        "48": "icons/48.png"
    },
    "web_accessible_resources": [
        "icons/48.png"
    ]
}
